<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="StateProgressBar">
        <attr name="bgColor" format="reference"/>
        <attr name="progressColor" format="reference"/>
        <attr name="successColor" format="reference"/>
    </declare-styleable>

    <declare-styleable name="BottomNavigatorView">
        <attr name="prevText" format="string" />
        <attr name="nextText" format="string" />
        <attr name="hintText" format="string" />
    </declare-styleable>

    <declare-styleable name="IconButton">
        <attr name="text" format="string" />
        <attr name="icon" format="reference" />
        <attr name="textColor" format="color" />
    </declare-styleable>
    
</resources>